<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://namespaces.github.com/jochenw/icm/icm-plugins/1.0.0" xmlns:tns="http://namespaces.github.com/jochenw/icm/icm-plugins/1.0.0" elementFormDefault="qualified">
  <xs:element name="icm-plugins">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="icm-plugin" minOccurs="1" maxOccurs="unbounded" type="tns:IcmPlugin"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="IcmPlugin">
    <xs:attribute name="class" type="xs:string" use="required"/>
  </xs:complexType>
</xs:schema>